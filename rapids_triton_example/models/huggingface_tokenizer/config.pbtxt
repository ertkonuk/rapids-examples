name: "huggingface_tokenizer"
backend: "python"
input [
 {
    name: "product_reviews"
    data_type: TYPE_STRING
    dims: [-1]
  }
]
output [
   {
    name: "input_ids"
    data_type: TYPE_INT32
    dims: [ -1, 256]
  },
  
   {
    name: "attention_mask"
    data_type: TYPE_INT32
    dims: [-1, 256]
  }
]

instance_group [{ kind: KIND_GPU }]

